{
  "name": "n8n-dev",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "node esbuild.js",
    "build:clean": "rimraf dist",
    "gen:clean": "rimraf generated",
    "clean": "npm run build:clean && npm run gen:clean",
    "start": "node -r esbuild-runner/register src/index.ts",
    "exec": "node -r esbuild-runner/register",
    "test": "jest",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "lintfix": "tslint --fix -p tsconfig.json -c tslint.json"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^28.1.4",
    "@types/lodash.mergewith": "^4.6.7",
    "@types/node": "^18.0.3",
    "esbuild": "^0.14.49",
    "esbuild-runner": "^2.2.1",
    "jest": "^28.1.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.5",
    "typescript": "^4.7.4"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testEnvironmentOptions": {
      "url": "http://localhost/"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testPathIgnorePatterns": [
      "/dist/",
      "/node_modules/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  },
  "dependencies": {
    "fs-extra": "^10.1.0",
    "lodash.mergewith": "^4.6.2",
    "tiny-glob": "^0.2.9"
  }
}
