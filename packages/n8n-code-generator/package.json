{
  "name": "@digital-boss/n8n-code-generator",
  "version": "1.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "node scripts/esbuild.js",
    "clean:build": "rimraf dist",
    "clean:gen": "rimraf generated",
    "clean": "npm run clean:build && npm run clean:gen",
    "exec": "node -r esbuild-runner/register",
    "test": "jest",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "lintfix": "tslint --fix -p tsconfig.json -c tslint.json",
    "version:bump": "npm version --no-commit-hooks --no-git-tag-version",
    "release": "fn(){ scripts/release.sh $1 n8n-code-generator; }; fn",
    "publish": "scripts/publish-private.sh dist/n8n-code-generator"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^28.1.4",
    "@types/lodash.mergewith": "^4.6.7",
    "@types/node": "^18.0.3",
    "esbuild": "^0.14.49",
    "esbuild-runner": "^2.2.1",
    "jest": "^28.1.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.5",
    "tslint": "^6.1.3",
    "typescript": "^4.7.4"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testEnvironmentOptions": {
      "url": "http://localhost/"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testPathIgnorePatterns": [
      "/dist/",
      "/node_modules/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  },
  "dependencies": {
    "fs-extra": "^10.1.0",
    "lodash.mergewith": "^4.6.2",
    "tiny-glob": "^0.2.9"
  }
}
