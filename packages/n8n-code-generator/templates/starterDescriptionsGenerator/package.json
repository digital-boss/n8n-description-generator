{
  "name": "@digital-boss/n8n-nodes-starter",
  "version": "0.1.0",
  "description": "FriendGrid support for n8n.",
  "license": "MIT",
  "homepage": "https://n8n.io",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/digital-boss/n8n-nodes-starter.git"
  },
  "keywords": [
    "n8n",
    "workflow",
    "n8n-community-node-package",
    "digital boss"
  ],
  "main": "index.js",
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc && gulp",
    "test": "jest",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "lintfix": "tslint --fix -p tsconfig.json -c tslint.json",
    "nodelinter": "nodelinter",
    "version:bump": "npm version --no-commit-hooks --no-git-tag-version",
    "version:gen": "genversion -s -e nodes/version.ts",
    "release": "./scripts/release.sh",
    "publish:pub": "npm publish --access=public",
    "publish:private": "./scripts/publish-private.sh",
    "publish": "npm run publish:private",
    "deploy": "./scripts/deploy.sh",
    "gen:build": "node gen-esbuild.js",
    "gen:run": "for i in nodes/*/descriptions/gen.js; do node $i; done",
    "gen:fix": "tslint --fix -p tsconfig.json -c tslint.json nodes/*/descriptions/generated/*.ts",
    "gen": "npm run gen:build && npm run gen:run && npm run gen:fix"
  },
  "files": [
    "dist"
  ],
  "n8n": {},
  "devDependencies": {
    "@digital-boss/n8n-node-generator": "github:digital-boss/n8n-node-generator#nodegen-v0.1.0",
    "@types/express": "^4.17.6",
    "@types/jest": "^27.4.0",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "14.17.27",
    "esbuild": "^0.14.47",
    "esbuild-runner": "^2.2.1",
    "genversion": "^3.1.1",
    "gulp": "^4.0.0",
    "jest": "^27.4.7",
    "js-yaml": "^4.1.0",
    "n8n-workflow": "~0.101.0",
    "nodelinter": "^0.1.9",
    "tiny-glob": "^0.2.9",
    "ts-jest": "^27.1.3",
    "ts-json-schema-generator": "^1.0.0",
    "tslint": "^6.1.2",
    "typescript": "~4.6.0"
  },
  "dependencies": {
    "@digital-boss/n8n-designpatterns": "0.1.8",
    "n8n-core": "~0.135.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testURL": "http://localhost/",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testPathIgnorePatterns": [
      "/dist/",
      "/node_modules/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  }
}
